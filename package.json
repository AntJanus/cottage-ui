{
  "name": "cottage-ui",
  "version": "0.1.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/cottage-ui.umd.cjs",
  "module": "./dist/cottage-ui.js",
  "exports": {
    ".": {
      "import": "./dist/cottage-ui.js",
